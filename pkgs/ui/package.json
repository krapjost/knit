{
  "name": "ui",
  "version": "0.0.0",
  "scripts": {
    "build": "vite build",
    "watcher": "vite build --watch",
    "dev": "pm2 start pnpm --name ui -- run watcher",
    "lint": "dprint fmt"
  },
  "types": "./src/index.ts",
  "files": ["dist"],
  "main": "./dist/ui.umd.js",
  "module": "./dist/ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/ui.es.js",
      "require": "./dist/ui.umd.js"
    },
    "./style.css": "./dist/style.css"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vanilla-extract/vite-plugin": "^3.8.0",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.2",
    "vite": "latest",
    "vite-plugin-solid": "latest"
  },
  "dependencies": {
    "@vanilla-extract/css": "^1.11.0",
    "normalize.css": "^8.0.1",
    "solid-js": "latest"
  }
}
